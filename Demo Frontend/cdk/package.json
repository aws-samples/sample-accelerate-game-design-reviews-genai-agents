{
  "name": "project-portal-cdk",
  "version": "1.0.0",
  "description": "CDK infrastructure for Project Portal with Amplify Gen 2",
  "main": "lib/index.js",
  "scripts": {
    "synth": "cdk synth",
    "synth:nag": "cdk synth 2>&1 | tee cdk-nag-report.txt",
    "nag": "npm run synth:nag",
    "deploy": "npx apmx sandbox",
    "generate-outputs": "ts-node scripts/generate-amplify-outputs.ts",
    "generate-outputs:dev": "ts-node scripts/generate-amplify-outputs.ts dev",
    "generate-outputs:prod": "ts-node scripts/generate-amplify-outputs.ts prod",
    "generate-outputs:shell": "bash scripts/generate-amplify-outputs.sh",
    "generate-graphql-types": "npx @aws-amplify/backend-cli generate graphql-client-code --out ../src/types/graphql-generated.ts"
  },
  "keywords": [
    "cdk",
    "amplify",
    "appsync",
    "cognito"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-amplify/backend": "^1.0.0",
    "@aws-cdk/aws-lambda-python-alpha": "^2.150.0-alpha.0",
    "@aws-sdk/client-cloudformation": "^3.600.0",
    "aws-cdk-lib": "^2.150.0",
    "cdk-nag": "^2.37.55",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@aws-amplify/backend-cli": "^1.8.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "aws-cdk": "^2.150.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0"
  }
}
